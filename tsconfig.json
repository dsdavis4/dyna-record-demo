  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "strictNullChecks": true,
    "strict": true,
    "target": "ES2022",
    "noImplicitOverride": true,
    "strictPropertyInitialization": false,
    "outDir": "./dist"
  }
}
